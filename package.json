    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mdb-react-ui-kit": "^6.1.0",
    "mongoose": "^7.3.1"
  },
  "devDependencies": {
